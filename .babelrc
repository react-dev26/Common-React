{
  "plugins": [
    "styled-jsx/babel",
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "es2015",
    "stage-0",
    "stage-1",
    "stage-2",
    "stage-3",
    "react",
    "next/babel"
  ]
}
