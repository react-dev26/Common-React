{
  "name": "cmn-website-node",
  "version": "1.0.0",
  "description": "Common website written with next.js",
  "main": "index.js",
  "scripts": {
    "ignore-trackers": "echo \"console.log('trackers are disabled in development')\" > static/trackers.min.js",
    "compile-trackers": "uglifyjs static/trackers/* -o static/trackers.min.js -c -m",
    "dev": "npm run ignore-trackers && NODE_PATH=./ node -r dotenv/config developmentServer.js",
    "build": "npm run compile-trackers && NODE_PATH=./ next build && NODE_PATH=./ next export",
    "start": "NODE_PATH=./ next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiCommon/cmn-website-node.git"
  },
